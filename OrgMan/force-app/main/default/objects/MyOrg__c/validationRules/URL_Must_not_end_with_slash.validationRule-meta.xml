<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>URL_Must_not_end_with_slash</fullName>
    <active>true</active>
    <errorConditionFormula>RIGHT (Login_URL__c, 1) = &#39;/&#39;</errorConditionFormula>
    <errorDisplayField>Login_URL__c</errorDisplayField>
    <errorMessage>Login URL must NOT end with &#39;/&#39;.</errorMessage>
</ValidationRule>
